<div class="inspires">
    {% for item in items %}
        <div class="inspirer-card">
            <img class="inspirer-card_bg lazy" data-src="{{ item.img }}" src="" alt="">
            <div class="inspirer-card_header">
                <h3>{{ item.title }}</h3>
                <p>{{ item.description }}</p>
            </div>
        </div>
    {% endfor %}
</div>
