<section class="slide with-overlay overlay-zindex_1" id="slide-tube" data-scroll>
    <img class="slide-img-bg lazy" src="assets/backgrounds/tube-bg.svg" alt=""
         data-src="assets/backgrounds/tube-bg.jpg">

    <input class="seek" step="1" type="range" min="0" max="100" value="0"/>
    <img data-src="assets/tube-left.png" src="" alt="" class="tube-parallax tube-left">
    <img data-src="assets/tube-right.png" src="" alt="" class="tube-parallax tube-right">
</section>
